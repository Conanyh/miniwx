.weui-tab__content {
  padding-top: 60px;
  text-align: center;
}
.title {
	text-align: center;
	position: relative;
	margin-top: 30rpx;
	margin-bottom: 40rpx;
   font-size: 16px;
  color: #515151;
}
.back-btn {
	position: absolute;
  font-size: 15px;
