.index-container {
  display: flex;
  flex-direction: row;
  justify-content: flex-start;
  box-sizing: border-box;
  flex-wrap: wrap;
  margin-top: 50rpx;
  margin-left: 43rpx;
}
.indexfont{
  font-family: YouYuan;
  font-size: 12px;
  position: absolute;
  top: 135rpx;
}

.icon-style {
  width: 46px;
  height: 46px;
}
.index-main-item-child{
  position: relative;
  width: 60px;
}
.index-main-item {
  height: 180rpx;
  width: 25%;
  margin: 10px 10px 15px 10px;
  border-radius: 16px;
  color: #58595e;
  display: flex;
  flex-direction: row;
  justify-content: center;
